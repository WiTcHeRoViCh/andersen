<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title><%= htmlWebpackPlugin.options.title %></title>
  </head>
  <body>

    <table id ="board_table">

    </table>

    <p id="mines_count"></p>

    <div id="custom_settings_div">
      <button id="custom_settings">Custom settings</button>

      <div id="settings">
        Row: <input type="field" id="row" value="2">
        </br>
        Col: <input type="field" id="col" value="2">
        </br>
        Mines: <input type="field" id="mines_field" value="2">

        <p></p>

        <button id="print_board_btn">Print custom board</button>

      </div>
    </div>


    <button id="easy">Easy</button>
    <button id="middle">Middle</button>
    <button id="hard">Hard</button>

    <button id="reset">Reset game</button>

    <span id="difficulty_message"></span>
  </body>
</html>